@model WebAutopark.Models.VehicleTypeViewModel
@{
    ViewData["Title"] = "Delete VehicleType";
}
<h2>@ViewBag.Title</h2>
<div>
    <dl class="dl-horizontal">
        <dt>Id</dt>
        <dd>
            @Model.TypeId
        </dd>

        <dt>Name</dt>
        <dd>
            @Model.TypeName
        </dd>

        <dt>TaxCoefficient</dt>
        <dd>
            @Model.TaxCoefficient
        </dd>
    </dl>

    <div>
        <form asp-controller="VehicleType" asp-action="Delete" asp-route-id="@Model.TypeId" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Delete" />
            </div>
        </form>
    </div>
</div>